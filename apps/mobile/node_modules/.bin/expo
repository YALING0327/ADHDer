else
  export NODE_PATH="/Users/lingyaliu/Downloads/ADHDer development/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__react-native@0.7_ghy2xvid2uqcabi3njkwtnhvxm/node_modules/expo/bin/node_modules:/Users/lingyaliu/Downloads/ADHDer development/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__react-native@0.7_ghy2xvid2uqcabi3njkwtnhvxm/node_modules/expo/node_modules:/Users/lingyaliu/Downloads/ADHDer development/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__react-native@0.7_ghy2xvid2uqcabi3njkwtnhvxm/node_modules:/Users/lingyaliu/Downloads/ADHDer development/node_modules/.pnpm/node_modules:$NODE_PATH"
